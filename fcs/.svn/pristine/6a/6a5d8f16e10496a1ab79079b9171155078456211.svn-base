void setup(){
  size(640,340);       //coordinate creation
  background(50);      //background color applied
  int number_a = 15;   //initialize number_a
  int number_b = 26;   //initialize number_b
  int C = 0;           //initialize C
  C = number_a * number_b;  //value of C is multiplication of number_a and number_b
  text("The multiplication of "+number_a+" and "+number_b+" is "+C,320,170);
    /* put the answer at the correct coordinate*/
}