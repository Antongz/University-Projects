void setup(){
  size(500,500);
  background(110,110,110);
  int a = 20;
  int n = 0;
  int m = 0;
  int v = 0;
  for(int i=0; i<=a; i++){
    if(i%2==0 && i%5!=0){fill(200,0,0);text(i,20,20+20*n);n = n + 1;
  }
    else if(i%2==0 && i%5==0){fill(0,200,0); text(i,40,20+20*m);m++;
  }
    else{fill(0,0,200);text(i,60,20+20*v);v++;}
  }
  fill(10,0,0);
  text("first ocurrence:",80,20);text(n,200,20);
  text("second ocurrence:",80,40);text(m,200,40);
  text("third ocurrence:",80,60);text(v,200,60);
}