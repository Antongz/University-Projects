public class Node{
	//Attributes:
	private int data; // the data saved in this node
	private Node left; // the reference to its left child
	private Node right; // the reference to its right child

	//constructors
	//default node data is -999
	public Node(){
		this.data = -999;
		left = null;
		right = null;
	}

	public Node(int data){
		this.data = data;
		left = null;
		right = null;
	}
	
	//mutators and accessors
	public int getData(){
		return data;
	}

	public Node getLeft(){
		return left;
	}

	public Node getRight(){
		return right;
	}

	public void setData(int data){
		this.data = data;
	}

	public void setLeft(Node left){
		this.left = left;
	}

	public void setRight(Node right){
		this.right = right;
	}

	//Methods:
	// Print the data saved in the node. 
	public void printNode(){
		System.out.println("Node data: "+"\""+ this.data + "\"");
	}
 
} 
