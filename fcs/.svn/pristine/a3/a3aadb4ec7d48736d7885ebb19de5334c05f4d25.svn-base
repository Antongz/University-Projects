//store all their family's information in an array
String [][] array = {{"Robert Stark","24","Lamb"},{"Ned Stark","44","Chicken"},{"Sansa Stark","18","Lettuce"},{"Arya Stark","16","Rabbit"},{"John Stark","17","Beans"}};

void setup(){
  size(500,500);
  text("Name",150,130);
  text("Age",250,130);
  text("Favourite food",280,130);
  fill(33);
  for(int i = 0; i<5; i++){        // using loop to display information on the canvas
    text(array[i][0],150,150+20*i);
    text(array[i][1],250,150+20*i);
    text(array[i][2],280,150+20*i);
  }
}